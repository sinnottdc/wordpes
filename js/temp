     function displayseed(seeds, seed_group) {

                var seed_offset = 0;

                if (seed_group = 2) {
                    seed_offset = 3;
                } else if (seed_group = 3) {
                    seed_offset = 7;
                } else if (seed_group = 4) {
                    seed_offset = 11;
                }
 
                for (i=0, i <= seeds; i++;) {        

                $('.seed_display').find('td').eq(i+seed_offset).text("working"+i+seed_offset);
            }

            }